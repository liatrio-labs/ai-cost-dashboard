{
  "buildCommand": "cd frontend && npm run build",
  "devCommand": "cd frontend && npm run dev",
  "installCommand": "cd frontend && npm install",
  "framework": "nextjs",
  "outputDirectory": "frontend/.next",
  "crons": [
    {
      "path": "/api/cron/anthropic",
      "schedule": "5 * * * *"
    },
    {
      "path": "/api/cron/openai",
      "schedule": "10 */6 * * *"
    },
    {
      "path": "/api/cron/refresh-aggregates",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron/forecasting",
      "schedule": "0 0 * * *"
    }
  ]
}
